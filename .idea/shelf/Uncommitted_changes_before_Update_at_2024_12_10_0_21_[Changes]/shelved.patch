ut type=\"text\" id=\"userName\" name=\"userName\" class=\"form-control\" placeholder=\"请输入账号\"\r\n                           required=\"true\">\r\n                </div>\r\n                <div class=\"form-group has-feedback\">\r\n                    <span class=\"fa fa-lock form-control-feedback\"></span>\r\n                    <input type=\"password\" id=\"password\" name=\"password\" class=\"form-control\" placeholder=\"请输入密码\"\r\n                           required=\"true\">\r\n                </div>\r\n\r\n                <div class=\"form-group has-feedback\"></div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-8\">\r\n                    </div>\r\n                    <div class=\"col-4\">\r\n                        <button type=\"submit\" class=\"btn btn-primary btn-block btn-flat\">登录\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </form>\r\n\r\n        </div>\r\n        <!-- /.login-card-body -->\r\n    </div>\r\n</div>\r\n<!-- /.login-box -->\r\n\r\n<!-- jQuery -->\r\n<script th:src=\"@{/admin/plugins/jquery/jquery.min.js}\"></script>\r\n<!-- Bootstrap 4 -->\r\n<script th:src=\"@{/admin/plugins/bootstrap/js/bootstrap.bundle.min.js}\"></script>\r\n<script th:src=\"@{/admin/dist/js/plugins/particles.js}\"></script>\r\n<script th:src=\"@{/admin/dist/js/plugins/login-bg-particles.js}\"></script>\r\n</body>\r\n</html>\r\n
